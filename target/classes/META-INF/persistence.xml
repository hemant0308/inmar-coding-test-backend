<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Inmar">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<jta-data-source>java:/DefaultDS</jta-data-source>
		<class>com.inmar.api.model.BaseModel</class>
		<class>com.inmar.api.model.Category</class>
		<class>com.inmar.api.model.Department</class>
		<class>com.inmar.api.model.Location</class>
		<class>com.inmar.api.model.SubCategory</class>
		<class>com.inmar.api.model.Token</class>
		<class>com.inmar.api.model.User</class>
		<class>com.inmar.api.model.Product</class>
	</persistence-unit>
</persistence>
